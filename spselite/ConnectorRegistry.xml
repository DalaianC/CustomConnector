<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE sailpoint PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<sailpoint>
	<ImportAction name="merge">
		<Configuration name="ConnectorRegistry">
			<Attributes>
			<Map>
	 			<entry key="applicationTemplates">
	 				<value>
	 					<List>
							<Application connector="sailpoint.connector.OpenConnectorAdapter" featuresString="PROVISIONING, DISCOVER_SCHEMA, ENABLE, SEARCH, PASSWORD" name="SPSElite" type="SPSElite">
							  <Attributes>
							    <Map>
							      <entry key="connectorClass" value="openconnector.SPSElite"/>
							      <entry key="formPath" value="SPSEliteConnectionForm.xhtml"/>
							    </Map>
							  </Attributes>
							  <Schemas>
							    <Schema displayAttribute="Username" identityAttribute="Username" nativeObjectType="account" objectType="account">
							      <AttributeDefinition name="Username" required="true" type="string"/>
							      <AttributeDefinition name="FirstName" required="true" type="string"/>
							      <AttributeDefinition name="LastName" required="true" type="string"/>
							      <AttributeDefinition name="EmailAddress" required="true" type="string"/>
							      <AttributeDefinition name="IsActive" required="true" type="boolean"/>
							    </Schema>
							  </Schemas>
							</Application>
							</List>
						</value>
					</entry>
				</Map>
			</Attributes>
		</Configuration>
	</ImportAction>
</sailpoint>